{
  "name": "memostory",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm run svg & vue-cli-service serve",
    "build": "npm run svg & vue-cli-service build",
    "lint": "vue-cli-service lint",
    "svg": "vsvg -s ./src/svg -t ./src/svg/js",
    "prerender-static": "cross-env NODE_ENV=production WITH_WIDGETS=true SERVER_URI='https://packhouse.ru' webpack --config prerender-static.js --progress --hide-modules",
    "prerender-catalog": "cross-env URL_CODE='catalog' NODE_ENV=production WITH_WIDGETS=true SERVER_URI='https://packhouse.ru' webpack --config prerender-catalog.js --progress --hide-modules",
    "prerender-products": "cross-env URL_CODE='products' NODE_ENV=production WITH_WIDGETS=true SERVER_URI='https://packhouse.ru' webpack --config prerender-catalog.js --progress --hide-modules",
    "prerender-reset": "cross-env URL_CODE='reset' NODE_ENV=production SERVER_URI='https://packhouse.ru' webpack --config prerender-catalog.js --progress --hide-modules"
  },
  "dependencies": {
    "@yzfe/vue-svgicon-loader": "^1.0.0-alpha.0",
    "axios": "^0.21.1",
    "axios-progress-bar": "^1.2.0",
    "core-js": "^3.6.5",
    "debounce": "^1.2.0",
    "headful": "^1.0.3",
    "js-md5": "^0.7.3",
    "moment": "^2.29.1",
    "swiper": "^5.4.5",
    "vue": "^2.6.11",
    "vue-awesome-swiper": "^4.1.1",
    "vue-cool-lightbox": "^2.7.4",
    "vue-lazyload": "^1.3.3",
    "vue-pull-refresh": "^0.2.7",
    "vue-router": "^3.3.4",
    "vue-scrollto": "^2.20.0",
    "vue-tel-input-vuetify": "^1.4.1",
    "vue-the-mask": "^0.11.1",
    "vuelidate": "^0.7.6",
    "vuetify": "^2.3.3",
    "vuetify-mask": "^1.0.0",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@mdi/font": "^5.9.55",
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-eslint": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cross-env": "^7.0.2",
    "eslint": "6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "install": "^0.13.0",
    "minify-css-string": "^1.0.0",
    "node-sass": "^4.14.1",
    "npm": "^6.14.9",
    "sass": "^1.26.9",
    "sass-loader": "^8.0.2",
    "terser-webpack-plugin": "^5.0.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue-cli-plugin-vuetify": "^2.0.6",
    "vue-spinner": "^1.0.4",
    "vue-svgicon": "3.2.6",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.5.0",
    "webpack-cli": "^3.3.12",
    "xmlhttprequest": "^1.8.0"
  }
}
